{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "My_PRIVATE_SECRET": "@my-private-secret",
    "MONGODB_URL": "@mongodb-privacy-url"
  },
  "github": {
    "enabled": true
  }
}
